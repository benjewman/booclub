<h1>Log In</h1>
<% if flash[:notice] %>
    <%= flash[:notice]%>
<%end%>

<%= form_with url: '/sessions' do |f| %>
    <%= f.label :username%>
    <%= f.text_field :username%>
    <%= f.label :password%>
    <%= f.password_field :password%>
    <%= f.submit%>
<%end%>
<br>

<%= button_to 'Sign up', signup_path, method: :get%>
<br>

<%= button_to 'Back to Books', books_path, method: :get %>